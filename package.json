    "@graphql-codegen/typescript-resolvers": "^1.9.1",
    "@types/dotenv": "^8.2.0",
    "@types/graphql": "^14.5.0",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.17",
    "@types/node-fetch": "^2.5.4",
    "@types/sqlite3": "^3.1.5",
    "cross-env": "^6.0.3",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "sqlite3": "^4.1.1",
